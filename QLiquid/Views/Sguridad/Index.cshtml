@{
    Layout = null;
}
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>QLiquid</title>
    <meta name="description" content="Marino, Admin theme, Dashboard theme, AngularJS Theme">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" href="favicon.ico">
    <!-- global stylesheets -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.1/animate.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/material-design-iconic-font/2.2.0/css/material-design-iconic-font.min.css" />
    <link href="https://fonts.googleapis.com/css?family=Roboto:400,300,300italic,400italic,500,500italic,700,700italic,900,900italic" rel="stylesheet" type="text/css" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/flag-icon-css/1.0.0/css/flag-icon.min.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="~/Content/css/main.css">
</head>
<body data-layout="empty-layout" data-palette="palette-0" data-direction="none">
    <div class="fullsize-background-image-1">
        <img src="~/Content/assets/backgrounds/bg845.png" />
    </div>

    <div class="container-fluid">
        <div class="row">
            <div class="col-xs-12">
                <div class="login-page text-center animated fadeIn delay-2000">
                    <h1>
                        Inicar Session
                    </h1>
                    <h4>
                        Porfavor ingrese sus credenciales para continuar
                    </h4>
                    <div class="row">
                        <div class="offset-xs-2 col-xs-8 offset-sm-3 col-sm-6 offset-md-3 col-md-6 offset-lg-4 col-lg-4">
                            <form id="frmMain" action="~/Home/Index" name="form">
                                <div class="row">
                                    <div class="col-xs-12">
                                        <div class="form-group floating-labels">
                                            <label for="email">Usuario</label>
                                            <input id="email" autocomplete="off" type="text" name="email">
                                            <p class="error-block"></p>
                                        </div>
                                    </div>
                                </div>
                                <div class="row m-b-40">
                                    <div class="col-xs-12">
                                        <div class="form-group floating-labels">
                                            <label for="password">Contraseña</label>
                                            <input id="password" autocomplete="off" type="password" name="password">
                                            <p class="error-block"></p>
                                        </div>
                                    </div>
                                </div>
                                <div class="row buttons">
                                    <div class="col-xs-12 col-md-6">
                                        <input type="button" class="btn-login btn-info" value="Ingresar" onclick="window.location.href = 'Home/TransferenciaExpress';">
                                    </div>
                                </div>
                            </form>
                        </div>
                    </div>


                </div>
            </div>
        </div>
    </div>


    <!-- global scripts -->
    <script src="~/Content/bower_components/jquery/dist/jquery.js"></script>
    <script src="~/Content/bower_components/tether/dist/js/tether.js"></script>
    <script src="~/Content/bower_components/bootstrap/dist/js/bootstrap.js"></script>
    <script src="~/Content/bower_components/PACE/pace.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/lodash.js/4.0.0/lodash.min.js"></script>
    <script src="~/Content/scripts/components/jquery-fullscreen/jquery.fullscreen-min.js"></script>
    <script src="~/Content/bower_components/jquery-storage-api/jquery.storageapi.min.js"></script>
    <script src="~/Content/bower_components/wow/dist/wow.min.js"></script>
    <script src="~/Content/scripts/functions.js"></script>
    <script src="~/Content/scripts/colors.js"></script>
    <script src="~/Content/scripts/left-sidebar.js"></script>
    <script src="~/Content/scripts/navbar.js"></script>
    <script src="~/Content/scripts/horizontal-navigation-1.js"></script>
    <script src="~/Content/scripts/horizontal-navigation-2.js"></script>
    <script src="~/Content/scripts/horizontal-navigation-3.js"></script>
    <script src="~/Content/scripts/main.js"></script>
    <script src="~/Content/scripts/components/floating-labels.js"></script>
    <script src="~/Content/scripts/pages-login.js"></script>
</body>
</html>
<script type="text/javascript">
    $("#frmMain").submit(function (event) {
        console.log("se envia el formulario");
        event.preventDefault();
    });
</script>